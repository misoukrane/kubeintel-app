{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:default",
    "fs:default",
    "fs:read-all",
    {
      "identifier": "fs:read-all",
      "allow": [
        "**/*"
      ]
    },
    "process:default"
  ]
}